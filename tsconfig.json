{
  "$schema": "https://json.schemastore.org/tsconfig",
  "_version": "22.0.0",
  "compilerOptions": {
    "module": "NodeNext",
    "target": "es2017",
    "noEmit": false,
    "outDir": "./dist/",
    "strict": false,
    "esModuleInterop": true,
    "moduleResolution": "nodenext",
    "strictNullChecks": true,
    "skipLibCheck": true,
    "types": ["node"]
  },
  "include": ["src/**/*"]
}
